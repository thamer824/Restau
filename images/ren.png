<script>(function () {
  var newatlas = window.newatlas || {};

  function truncate(selector, maxLines) {
    // there are multiple issues with IE11
    if (!Number.isInteger) return;

    var els = document.querySelectorAll(selector || '');
    newatlas.log(
      'Truncating: ' +
        selector +
        ', maxLines: ' +
        maxLines +
        ', found: ' +
        els.length
    );
    if (!els.length) return;

    maxLines = Number.isInteger(maxLines) ? maxLines : 5;

    Array.from(els).forEach(function (el) {
      var box = el.getBoundingClientRect();
      var content = el.textContent;
      var styles = window.getComputedStyle(el);
      var fontSize = styles.getPropertyValue('font-size');
      var lineHeight = styles.getPropertyValue('line-height');
      var maxHeight = maxLines * parseInt(lineHeight, 10);
      var words = content.split(' ');

      var dummy = document.createElement('div');
      dummy.id = 'truncate_' + Date.now();
      dummy.style.position = 'absolute';
      dummy.style.width = box.width + 'px';
      dummy.style.visibility = 'hidden';
      dummy.style.fontSize = fontSize;
      dummy.style.lineHeight = lineHeight;
      dummy.textContent = content;
      el.append(dummy);

      var currHeight = dummy.offsetHeight;
      if (currHeight <= maxHeight) {
        document.getElementById(dummy.id).remove();
        return;
      }

      // start drop one word on the first pass
      var currWords = words.length - 1;
      var truncated = '';

      while (currHeight > maxHeight) {
        truncated =
          words.slice(0, currWords).join(' ').trim().replace(/\W$/, '') + '…';
        dummy.textContent = truncated;
        currHeight = dummy.offsetHeight;
        currWords--;
      }

      // setting innerText overwrites the dummy element
      el.innerText = truncated.trim().replace(/\W$/, '');
      el.insertAdjacentHTML(
        'beforeend',
        '<span title="…' + content.slice(truncated.length, -1) + '">…</span>'
      );
      newatlas.log('Truncating description', el);
    });
  }

  document.addEventListener('DOMContentLoaded', function () {
    // See also CSS Only truncation in ad-hoc-styles...
    var isMobile = window.innerWidth < 768;

    if (isMobile) {
      // homepage first lead
      truncate('.HomePage-content .Page-pageLead .PromoC-description', 6);

      // Homepage lead 2nd,3rd
      truncate('.HomePage-content .Page-pageLead .PromoB-description', 6);

      // Homepage Single col Promo List descriptions
      truncate('.HomePage-content .ListE .PromoB-description', 6);

      // 3 Col Latest News descriptions on Section pages
      truncate('.ListD .PromoB-description', 6);
    } else {
      // homepage first lead
      truncate('.HomePage-content .Page-pageLead .PromoC-description', 5);

      // Homepage lead 2nd,3rd
      truncate('.HomePage-content .Page-pageLead .PromoB-description', 4);

      // Homepage Single col Promo List descriptions
      truncate('.HomePage-content .ListE .PromoB-description', 5);

      // 3 Col Latest News descriptions on Section pages
      truncate('.ListD .PromoB-description', 5);
    }
  });
})();</script>
